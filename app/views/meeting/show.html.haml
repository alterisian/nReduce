- content_for :subtitle do
  #{@meeting.location_name} Meeting

%h2 #{@meeting.location_name} Meeting

- if user_signed_in? and current_user.id == meeting.organizer_id
  .row
    .span12
      = semantic_form_for @startup, :url => '/startup/', :html => {:class => "form-horizontal"} do |f|

        = render "startups/fields", :f => f

        .form-actions
          = f.submit "Save", :class => "btn btn-primary btn-large"
