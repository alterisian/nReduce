<h1>
  <%= link_to "← back", "/admin/startups", :class => "back btn" %>
  <%= @startup.name %>
</h1>
<br>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th class="yellow">Team Member</th>
    </tr>
  </thead>
  <tbody>
    <% @startup.team_members.each do |team_member| %>
      <tr>
        <td>
          <%= link_to "#{team_member}", "http://twitter.com/#{team_member.gsub("@", "")}" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>